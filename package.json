{
  "name": "navicode",
  "displayName": "Navicode",
  "description": "Navigate and interact with GPT API for project insights.",
  "version": "0.1.1",
  "publisher": "baoyuanl",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:navicode.openPrompt",
    "onCommand:navicode.preprocessEmbeddings"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "navicode.openPrompt",
        "title": "Open Navicode Prompt"
      },
      {
        "command": "navicode.preprocessEmbeddings",
        "title": "Preprocess File Embeddings"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Navicode Configuration",
      "properties": {
        "navicode.apiKey": {
          "type": "string",
          "description": "Your GPT API key",
          "default": ""
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/madge": "^5.0.3",
    "@types/node": "^14.14.6",
    "typescript": "^4.0.3",
    "vscode": "^1.1.34"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "madge": "^4.0.2"
  }
}
